{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@components/*": [ "./src/components/*" ],
      "@assets/*": [ "./src/assets/*" ],
      "@lib/*": [ "./src/lib/*" ],
      "@layouts/*": [ "./src/layouts/*" ],
      "@firebase/*": [ "./firebase/*" ],
      "@scripts/*": [ "./src/scripts/*" ]
    }
  }
}
